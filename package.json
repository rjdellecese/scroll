    "start": "convex dev --once && concurrently --restart-tries=-1 --prefix-colors=\"black.bold.bgCyan,black.bold.bgYellow\" --prefix=\" {name} \" --names=\"PARCEL,CONVEX\" \"parcel serve src/elm-ts/index.html\" \"convex dev\"",
